
<%= form_with url:new_account_details_path do |form|%>
    <%=form.hidden_field :project_id ,value:@project_id.to_i %>
    <%=form.hidden_field :member_id ,value:@member_id.to_i %>
    <div class="form-group mb-3">
        <%=form.label :account_number,class:"form-label"%>
        <%=form.text_field :account_number ,class:"form-control",required:true %>
    </div>
    <div class="form-group mb-3">
        <%=form.label :ifsc_code,class:"form-label"%>
        <%=form.text_field :ifsc_code ,class:"form-control",required:true %>
    </div>
    <div>
        <%=form.submit "Verify and Submit",class:"btn btn-success" %>
    </div>
<% end %>
