<%= form_with model: [@freelancer ,@freelancer.skills.build ],url:new_freelancer_skill_path do |form|%>

<p>
  <%=form.hidden_field "to", value:@to%>
  <%=form.hidden_field "from",value:@from%>
</p>

<p>
    <%= form.label :name%><br>
    <%= form.text_field :name%>
</p>

<p>
  <%= form.label :level %><br>
  <%= form.select :level, ['Beginner', 'Intermediate', 'Advanced'], selected: 'Intermediate' %>
</p>

<p>
    <%= form.submit %>
</p>

<%end%>
